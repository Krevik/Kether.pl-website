{
	"name": "kether.pl",
	"version": "0.3.0",
	"private": true,
	"homepage": "https://kether.pl",
	"dependencies": {
		"@reduxjs/toolkit": "2.6.1",
		"@types/node": "^22.18.12",
		"@types/react": "19.0.11",
		"@types/react-dom": "19.0.4",
		"gh-pages": "^6.3.0",
		"i18next": "^25.6.0",
		"i18next-browser-languagedetector": "^8.2.0",
		"primereact": "10.9.3",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-i18next": "^15.7.4",
		"react-redux": "9.2.0",
		"redux-persist": "^6.0.0",
		"terser": "^5.44.0",
		"typescript": "^5.9.3"
	},
	"scripts": {
		"dev": "vite",
		"start": "vite",
		"build": "tsc && vite build && shx cp build/index.html build/404.html",
		"build:dev": "tsc && vite build --mode development && shx cp build/index.html build/404.html",
		"preview": "vite preview",
		"type-check": "tsc --noEmit",
		"lint": "eslint src --ext .ts,.tsx",
		"lint:fix": "eslint src --ext .ts,.tsx --fix",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build"
	},
	"browserslist": {
		"production": [
			">0.5% in alt-EU",
			"unreleased Chrome versions",
			"unreleased Firefox versions",
			"not dead",
			"not iOS <= 2137",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^8.46.2",
		"@typescript-eslint/parser": "^8.46.2",
		"@vitejs/plugin-react": "^5.0.4",
		"eslint": "^9.38.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.24",
		"prettier": "3.5.3",
		"react-router-dom": "^7.9.4",
		"shx": "^0.4.0",
		"typescript-plugin-css-modules": "^5.2.0",
		"vite": "^6.4.1",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"prettier": {
		"trailingComma": "es5",
		"tabWidth": 4,
		"semi": true,
		"singleQuote": true,
		"indent_size": 2,
		"max_line_length": 160
	}
}
