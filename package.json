{
	"name": "kether.pl",
	"version": "0.2.1.0",
	"private": true,
	"homepage": "https://kether.pl",
	"dependencies": {
		"@reduxjs/toolkit": "2.6.1",
		"@types/node": "^22.17.2",
		"@types/react": "19.0.11",
		"@types/react-dom": "19.0.4",
		"axios": "^1.11.0",
		"gh-pages": "^6.3.0",
		"i18next": "^25.4.0",
		"i18next-browser-languagedetector": "^8.2.0",
		"i18next-http-backend": "^3.0.2",
		"prettier": "3.5.3",
		"primeicons": "7.0.0",
		"primereact": "10.9.3",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-i18next": "^15.7.1",
		"react-redux": "9.2.0",
		"react-scripts": "^5.0.1",
		"redux-persist": "^6.0.0",
		"typescript": "^5.9.2"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "GENERATE_SOURCEMAP=false react-scripts build && shx cp build/index.html build/404.html",
		"build:dev": "GENERATE_SOURCEMAP=true react-scripts build && shx cp build/index.html build/404.html",
		"build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
		"build:analyze:dev": "npm run build:dev && npx webpack-bundle-analyzer build/static/js/*.js",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
		"runBoth": "run-p server start",
		"lint": "eslint src --ext .ts,.tsx",
		"lint:fix": "eslint src --ext .ts,.tsx --fix",
		"type-check": "tsc --noEmit",
		"performance:analyze": "npm run build && npx lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
		"performance:budget": "npm run build && npx bundle-buddy build/static/js/*.js"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.5% in alt-EU",
			"unreleased Chrome versions",
			"unreleased Firefox versions",
			"not dead",
			"not iOS <= 2137",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"react-router-dom": "^7.8.1",
		"shx": "^0.4.0",
		"typescript-plugin-css-modules": "^5.2.0"
	},
	"prettier": {
		"trailingComma": "es5",
		"tabWidth": 4,
		"semi": true,
		"singleQuote": true,
		"indent_size": 2,
		"max_line_length": 160
	}
}
